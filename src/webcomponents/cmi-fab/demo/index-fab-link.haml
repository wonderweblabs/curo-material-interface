!!!
%html
  %head
    %meta{:charset => "utf-8"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => "width=device-width, minimum-scale=1.0, initial-scale=1, user-scalable=yes", :name => "viewport"}
    %title cmi-fab-link demo
    %script{:src => "../../webcomponentsjs/webcomponents-lite.js"}
    %link{:href => "../../cmi-doc-code-box/cmi-doc-code-box.html", :rel => "import"}
    %link{:href => "../../iron-icons/iron-icons.html", :rel => "import"}
    %link{:href => "../cmi-fab-link.html", :rel => "import"}
    %link{:href => "../../paper-styles/demo-pages.html", :rel => "import"}
  %body{ unresolved: true }
    .vertical.center-justified.layout

      .vertical-section
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', noink: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', toggles: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', disabled: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', theme: 'primary' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', theme: 'highlight' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', theme: 'danger' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', filled: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', filled: true, theme: 'primary' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', filled: true, theme: 'highlight' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', filled: true, theme: 'danger' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, theme: 'primary' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, theme: 'highlight' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, theme: 'danger' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, filled: true }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, filled: true, theme: 'primary' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, filled: true, theme: 'highlight' }
            %iron-icon{ icon: 'done' }
        %cmi-doc-code-box{ :'left-cols' => '4', :'right-cols' => '8' }
          %a{ is: 'cmi-fab-link', raised: true, filled: true, theme: 'danger' }
            %iron-icon{ icon: 'done' }
