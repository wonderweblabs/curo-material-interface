            %cmi-card-actions{ 'line-top' => true }
              %cmi-button Do this
              %cmi-button Do that

      .vertical-section
        %cmi-doc-code-box{ 'left-cols' => '12', 'right-cols' => '12' }
          %cmi-card{ theme: 'highlight' }
            %cmi-card-heading
              %h2.title Some title
            %cmi-card-content
